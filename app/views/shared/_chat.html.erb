<div class="chat-box span4">
<ul id="messages"></ul>
<form class="row" action="">
  <input id="m" autocomplete="off" class="span3" /><button>Send</button>
</form>
</div>
<script>
  var socket = io('http://localhost:8080');
  $('form').submit(function(){
    socket.emit('chat message', "me: " + $('#m').val());
    $('#m').val('');
    return false;
  });
  socket.on('chat message', function(msg){
    $('#messages').append($('<li>').text(msg));
  });
</script>