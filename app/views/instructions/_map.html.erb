<script type="text/javascript">
  $(document).ready(function(){
    //var address = new google.maps.LatLng(10.757868,106.667488);
	var address = new google.maps.LatLng(10.781588,106.672912);
    var mapOptions = {
      center: address,
      zoom: 16,
      mapTypeId: google.maps.MapTypeId.ROADMAP
    };
    var map = new google.maps.Map(document.getElementById("map_canvas"),mapOptions);
    
    var marker = new google.maps.Marker({
      position: address,
      title:"Shop muamely"
    });
    
    marker.setMap(map);
  });
</script>